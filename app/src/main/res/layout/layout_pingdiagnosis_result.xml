<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">
    <LinearLayout
        android:id="@+id/pingdiag_lin_choiceWAN"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal">
        <ImageView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:scaleType="centerInside"
            android:src="@mipmap/icon_label"/>
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/str_thisdiagnosisresult"
            android:gravity="center"
            android:textSize="@dimen/sp_16"
            android:textColor="@color/gray_deep"/>


    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/sp_16"
        android:background="@color/gray_f2"></View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/pingaddress"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_host"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/pingtimeout"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_numberOfRepetitions"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/timeout"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_timeout"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/pingpakgesize"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_dataBlockSize"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/successtime"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_successCount"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/defauttime"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_failureCount"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/averageresponstime"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_averageResponseTime"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/minimumresponsetime"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_minimumResponseTime"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_50"
        android:orientation="horizontal"
        android:gravity="center_vertical">
        <TextView
            android:layout_marginLeft="@dimen/dp_16"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/maxresponsetime"
            android:textColor="@color/gray_deep"
            android:textSize="@dimen/sp_16"/>

        <TextView
            android:id="@+id/tv_maximumResponseTime"
            android:layout_marginRight="@dimen/dp_16"
            android:layout_alignParentRight="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text=" "
            android:textColor="@color/gray_light"
            android:textSize="@dimen/sp_16"/>


    </RelativeLayout>

    <View
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/deep_line">

    </View>


</LinearLayout>
