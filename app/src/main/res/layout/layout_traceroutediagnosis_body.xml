<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/gray_f2">

        <LinearLayout
            android:id="@+id/trace_lin_choiceWAN"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_50"
            android:orientation="horizontal">
            <ImageView
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:scaleType="centerInside"
                android:src="@mipmap/icon_label"/>
            <TextView
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/choiceWAN"
                android:gravity="center"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/gray_deep"/>

            <TextView
                android:id="@+id/tv_connectionType"
                android:layout_marginRight="@dimen/dp_8"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="match_parent"
                android:text=""
                android:gravity="center_vertical|end"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/gray_deep"/>

            <ImageView
                android:layout_marginRight="@dimen/dp_16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:src="@mipmap/icon_enter"/>
        </LinearLayout>

        <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_16"
            android:background="@color/gray_f2"></View>


        <LinearLayout
            android:id="@+id/lin_set"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_50"
            android:orientation="horizontal">
            <ImageView
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:scaleType="centerInside"
                android:src="@mipmap/icon_label"/>
            <TextView
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/setupparameters"
                android:gravity="center"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/gray_deep"/>


        </LinearLayout>
        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="@dimen/px_1"
            android:background="@color/deep_line"></View>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:background="@color/white">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="@string/protocoltype"/>

                <LinearLayout
                    android:id="@+id/lin_modechoice"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_modechoice"
                        android:layout_width="0dp"
                        android:layout_weight="1"
                        android:layout_height="match_parent"
                        android:gravity="center_vertical|right"
                        android:text="UDP"
                        android:textColor="@color/gray_light"
                        android:textSize="@dimen/sp_14"
                        android:layout_marginRight="@dimen/dp_8"/>
                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:src="@mipmap/icon_pull"/>
                </LinearLayout>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="@string/wanaddress"/>

                <EditText
                    android:id="@+id/edt_host"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:hint="请输入链接地址"
                    android:textColor="@color/gray_deep"
                    android:textColorHint="@color/gray_light"
                    android:inputType="textWebEmailAddress"
                    android:singleLine="true"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/sp_14"
                    android:text="www.baidu.com"/>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="每跳重复次数"/>

                <EditText
                    android:id="@+id/edt_numberOfTries"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:hint="请输入每跳重复次数"
                    android:textColor="@color/gray_deep"
                    android:textColorHint="@color/gray_light"
                    android:inputType="number"
                    android:singleLine="true"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/sp_14"
                    android:text="3"/>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="@string/timeout"/>

                <EditText
                    android:id="@+id/edt_timeout"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:hint="清输入超时时间(毫秒)"
                    android:textColor="@color/gray_deep"
                    android:textColorHint="@color/gray_light"
                    android:inputType="number"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/sp_14"
                    android:singleLine="true"
                    android:text="5000"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="@string/packetsize"/>

                <EditText
                    android:id="@+id/edt_dataBlockSize"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:hint="请输入包大小(字节)"
                    android:textColor="@color/gray_deep"
                    android:textColorHint="@color/gray_light"
                    android:inputType="number"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/sp_14"
                    android:singleLine="true"
                    android:text="38"/>


            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="@string/dscp"/>

                <EditText
                    android:id="@+id/edt_dscp"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:hint="请输入DSCP"
                    android:textColor="@color/gray_deep"
                    android:textColorHint="@color/gray_light"
                    android:inputType="number"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/sp_14"
                    android:singleLine="true"
                    android:text="0"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:orientation="horizontal"
                android:gravity="center_vertical">
                <TextView
                    android:layout_marginLeft="@dimen/dp_16"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/gray_deep"
                    android:text="@string/numberoftimes"/>

                <EditText
                    android:id="@+id/edt_maxHopCount"
                    android:layout_marginRight="@dimen/dp_16"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="match_parent"
                    android:background="@null"
                    android:gravity="center_vertical|end"
                    android:hint="请输入最大跳数"
                    android:textColor="@color/gray_deep"
                    android:textColorHint="@color/gray_light"
                    android:inputType="number"
                    android:textCursorDrawable="@null"
                    android:textSize="@dimen/sp_14"
                    android:singleLine="true"
                    android:text="30"/>

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginLeft="@dimen/dp_16"
                android:background="@color/deep_line"></View>
            <Button
                android:id="@+id/trace_butt_start"
                android:layout_marginLeft="@dimen/dp_16"
                android:layout_marginRight="@dimen/dp_16"
                android:layout_marginTop="@dimen/dp_16"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_50"
                android:gravity="center"
                android:text="@string/startdiagnosis"
                android:textSize="@dimen/sp_16"
                android:textColor="@color/white"
                android:background="@drawable/bg_blue_all_4radius"/>
        </LinearLayout>


</LinearLayout>
